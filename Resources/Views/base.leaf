<!DOCTYPE html>
<html lang="en-US">
    
    <head>
        <meta charset="UTF-8" />
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
        <script src="https://cdn.jsdelivr.net/npm/@fancyapps/ui@4.0/dist/fancybox.umd.js"></script>
        <script src="/scripts/scripts.js"></script>
        <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/@fancyapps/ui@4.0/dist/fancybox.css" type="text/css" />
        <link rel="stylesheet" href="/styles/style.css" type="text/css" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <link rel="canonical" href="#(meta.canonical)" />

        <title>#(meta.title) | #(meta.siteName)</title>
        <meta name="author" content="#(meta.author)"/>
        <meta name="description" content="#(meta.description)" />
        <!-- open graph -->
        <meta name="og:site_name" content="#(meta.siteName)" />
        <meta name="og:title" content="#(meta.title) | #(meta.siteName)" />
        <meta name="og:description" content="#(meta.description)" />
        <meta name="og:url" content="#(meta.canonical)" />
        <!-- twitter -->
        <meta name="twitter:url" content="#(meta.canonical)" />
        <meta name="twitter:title" content="#(meta.title) | #(meta.siteName)" />
        <meta name="twitter:card" content="#(meta.author)" />
        <meta name="twitter:description" content="#(meta.description)" />
        <!-- font -->
        <link rel="preconnect" href="https://fonts.googleapis.com"> 
        <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin> 
        <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Questrial&display=swap">
        <!-- favicon -->
        <link rel="icon" href="/favicon-32.png" sizes="32x32" />
        <link rel="icon" href="/favicon-128.png" sizes="128x128" />
        <link rel="icon" href="/favicon-152.png" sizes="152x152" />
        <link rel="icon" href="/favicon-167.png" sizes="167x167" />
        <link rel="icon" href="/favicon-180.png" sizes="180x180" />
        <link rel="icon" href="/favicon-192.png" sizes="192x192" />
        <link rel="icon" href="/favicon-196.png" sizes="196x196" />
    </head>
    
    <body>
        <nav>
            <div class="nav-side left">
                <div class="logo">
                    <a href="/">
                        <img src="logo.svg" alt="tinmey logo">
                    </a>
                </div>
            </div>
            <div class="nav-side right">
                <div class="contact">
                    <a href="mailTo:#(meta.email)">
                        <img src="icon-mail.svg" alt="mail icon">
                    </a>
                </div>
                <div class="contact">
                    <a href="https://medium.com/@katyatinmey" target="_blank">
                        <img src="icon-medium.svg" alt="instagram icon">
                    </a>
                </div>
                <div class="contact">
                    <a href="https://www.instagram.com/tinmeydesign" target="_blank">
                        <img src="icon-instagram.svg" alt="instagram icon">
                    </a>
                </div>
                <!-- <div class="contact">
                    <a href="https://www.behance.net/katyatinmey" target="_blank">
                        <img src="icon-behance.svg" alt="behance icon">
                    </a>
                </div> -->
            </div>
        </nav>

        <header>
            <div class="wrapper">
                #extend("header")
            </div>
        </header>
        <div class="wrapper">
            #import("--content")
        </div>
        <footer>
            <p>
                Â© 2022 tinmey design
                <br>
                Developed by <a href="https://www.dmitry-borodin.com" target="_blank">Dmitry Borodin</a>
            </p>
        </footer>
    </body>
    
</html>
